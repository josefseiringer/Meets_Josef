@model List<Meets.Models.DetailBesaetigungViewModel>

@{
    ViewBag.Title = "DetailEventBestaetigt";
    Layout = "~/Views/Shared/_LayoutGeladneDetail.cshtml";
}

@foreach (Meets.Models.DetailBesaetigungViewModel item in Model)
{
    <div class="backwitheEventsmaller">
        <table>
            <tr>
                <th>Eventdatum:</th>
                <td style="padding-left:10px;">@Html.DisplayFor(modelItem => item.Eventdatum)</td>
            </tr>
            <tr>
                <th>Titel:</th>
                <td style="padding-left:10px;">@Html.DisplayFor(modelItem => item.Eventtitel)</td>
            </tr>
            <tr>
                <th>Bestätigungszeitpunkt:</th>
                <td style="padding-left:10px;">@Html.DisplayFor(modelItem => item.Bestaetigungszeitpunkt)</td>
            </tr>
            <tr>
                <th>Eingeleadene E-Mail:</th>
                <td style="padding-left:10px;">@Html.DisplayFor(modelItem => item.Eingeladen)</td>
            </tr>
            <tr>
                <th>Angenommen Ja/Nein:</th>
                <td style="padding-left:10px;">@Html.DisplayFor(modelItem => item.Bestaetigt)</td>
            </tr>

        </table>
    </div>
}


